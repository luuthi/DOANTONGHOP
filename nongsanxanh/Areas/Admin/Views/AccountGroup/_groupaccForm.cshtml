@model Bussiness.ViewModel.GroupAccountViewModel
<div class="row" style="margin-bottom:5px; margin-right:0px;">
    @Html.ActionLink("Quay lại", "Index", "AccountGroup", null, new { @class = "btn btn-default", @style = "float:right; margin-right: 20px; margin-top: 10px" })
</div>
<div class="box-body">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                @Html.LabelFor(m => m.GroupCode)
                @Html.TextBoxFor(m => m.GroupCode, new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.GroupCode)
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                @Html.LabelFor(m => m.GroupName)
                @Html.TextBoxFor(m => m.GroupName, new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.GroupName)
            </div>
        </div>

    </div>
    <div class="row" style="float: right; padding-right: 20px">
        <button type="submit" class="btn btn-primary submit_add_post ">Ghi lại</button>
    </div>
    <!-- /.panel-body -->
</div>
<script>
            
</script>
